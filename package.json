{
  "name": "policy_desk",
  "version": "1.0.0",
  "description": "It's a RESTful API application for Policy Desk with Node.js, Express, and PostgreSQL.",
  "main": "index.js",
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "start": "nodemon index.js --exec babel-node --presets env",
    "serve": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PalashBera/policy_desk.git"
  },
  "keywords": [
    "RESTful API",
    "Express",
    "PostgreSQL",
    "Node.js",
    "Policy"
  ],
  "author": "Palash Bera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PalashBera/policy_desk/issues"
  },
  "homepage": "https://github.com/PalashBera/policy_desk#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1"
  }
}
